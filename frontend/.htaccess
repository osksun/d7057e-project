RewriteEngine On
RewriteRule ^$ /src/index/html/index.php [L]
RewriteRule ^courses/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^courses/([^/]+)/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^createcourse$ /src/index/html/index.php [L]
RewriteRule ^createmodule/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^createquestion/([^/]+)/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^editcourse/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^moderators/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^editmodule/([^/]+)/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^editquestion/([^/]+)/([^/]+)/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^questionlist/([^/]+)/([^/]+)$ /src/index/html/index.php [L]
RewriteRule ^settings$ /src/settings/settings.php [L]
RewriteRule ^login$ /src/register/login.php [L]
RewriteRule ^register$ /src/register/register.php [L]

ErrorDocument 403 /src/404/404.php
ErrorDocument 404 /src/404/404.php
